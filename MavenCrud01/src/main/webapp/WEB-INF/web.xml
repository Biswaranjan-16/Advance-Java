<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>EmployeeServlet</servlet-name>
  	<display-name>EmployeeServlet</display-name>
  	<description></description>
  	<servlet-class>com.biswa.controller.EmployeeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ViewEmployeeServlet</servlet-name>
  	<display-name>ViewEmployeeServlet</display-name>
  	<description></description>
  	<servlet-class>com.biswa.controller.ViewEmployeeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteEmployeeServlet</servlet-name>
  	<display-name>DeleteEmployeeServlet</display-name>
  	<description></description>
  	<servlet-class>com.biswa.controller.DeleteEmployeeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>EmployeeServlet</servlet-name>
  	<url-pattern>/EmployeeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ViewEmployeeServlet</servlet-name>
  	<url-pattern>/ViewEmployeeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteEmployeeServlet</servlet-name>
  	<url-pattern>/DeleteEmployeeServlet</url-pattern>
  </servlet-mapping>
</web-app>
